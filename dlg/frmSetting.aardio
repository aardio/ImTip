			else {
				font.name = fontFamily.getName() 
			} 
			
			frmSetting.preview.iconStyle.font = ::LOGFONT(font);
			frmSetting.preview.redraw();	
			
			if(!imeBar.iconStyle) imeBar.iconStyle = {}
			imeBar.iconStyle.font = ::LOGFONT(font);
			imeBar.redraw() 
		}
	}
