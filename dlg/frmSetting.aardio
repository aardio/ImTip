		capital = unescapeColor(frmSetting.editCharsCapital.text);
		symbol = unescapeColor(frmSetting.editCharsSymbol.text);
		close = unescapeColor(frmSetting.editCharsClose.text); 
		[0x409] = unescapeColor(frmSetting.editCharsEn.text);
		[0x804] = unescapeColor(frmSetting.editCharsLang.text);
	});
	
	if(owner==frmSetting.editCharsLang.editBox){ 
		if(frmSetting.tbsPreviewMode.selIndex = 1 ){
			frmSetting.preview.text =  unescapeColor(owner.text);	
		}
	}
	elseif(owner==frmSetting.editCharsEn.editBox){
		if(frmSetting.tbsPreviewMode.selIndex = 2 ){
