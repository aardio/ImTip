import gdip.snap; 
import win.debounce;
frmSetting.updateSchemeThumbnail = win.debounce( function(schemeIndex){ 
 
	if(!config.setting.schemeThumbnails) config.setting.schemeThumbnails = {}; 
	var ctrl = frmSetting["btnScheme" + schemeIndex ]; 
	
	var bmp = gdip.snap(frmSetting.preview.hwnd)
 
 	var x,y,cx,cy = ctrl.getPos();
 	var dpiScaleX = ctrl.dpiScaleX : 1;
 	if(dpiScaleX>1){
 		var img = bmp.getThumbnail(bmp.width/dpiScaleX,bmp.height/dpiScaleX,true);
 		config.setting.schemeThumbnails[schemeIndex] = img.saveToBuffer();
 		img.dispose();
 	}
 	else {
 		config.setting.schemeThumbnails[schemeIndex]  = bmp.saveToBuffer();
 	} 
 	
	if(bmp.width>cx || bmp.height>cy){
		ctrl.foreRepeat = "scale"
	}
	else {
		ctrl.foreRepeat = "center"	
	}
	bmp.dispose();
	
	ctrl.text = "";
	var prevBmp = ctrl.setForeground(config.setting.schemeThumbnails[schemeIndex],false);
	if(prevBmp){
		prevBmp.dispose();
	}
},1000);

