frmSetting.tbInterval.onPosChanged = function( pos,triggeredByUser ){  
	
	if( pos < 10 ) tip = "（极快，流畅）";
	elseif( pos < 40 ) tip = "（略快，流畅）";
	elseif( pos < 60 ) tip = "（适中，流畅 + 节能）";
	elseif( pos < 120 ) tip = "（略慢，节能）";
	else tip = "（极慢，节能）";
	
	imeBar.interval = pos;
	frmSetting.lbInterval.text = pos + " 毫秒 " + tip; 	
}

